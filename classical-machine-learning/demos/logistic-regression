   "source": [
    "def cost_function(X, y, weights):\n",
    "    scores = np.dot(X, weights)\n",
    "    h = sigmoid(scores)\n",
    "    return (-1/len(df)) * sum( (y*np.log(h)) + (1-y)*(np.log(1-h)) )"
   ]
  },
