    "# Plot the decision boundary\n",
    "\n",
    "sns.scatterplot(x = df[\"Age\"], y = df[\"EstimatedSalary\"], hue = df.Purchased)\n",
    "\n",
    "# x1 reffers to Age column, x2 reffers to EstimatedSalary column\n",
    "x2_vals = df.EstimatedSalary\n",
    "x1_vals = -(theta[0] + theta[2] * x2_vals) / theta[1]\n",
    "plt.plot(x1_vals, x2_vals, 'r')\n",
    "\n",
    "print(x1_vals, x2_vals)\n",
    "\n",
    "plt.show()\n",
    "\n",
    "\n",
    "sns.scatterplot(x = df[\"Age\"], y = df[\"EstimatedSalary\"], hue = df.Purchased)\n",
    "\n",
    "\n",
    "x1 = (-theta[0] - theta[2] * df.iloc[0,1])/theta[1]\n",
    "x2 = (-theta[0] - theta[2] * df.iloc[4,1])/theta[1]\n",
    "\n",
    "plt.axline((x1, df.iloc[0,1]), (x2, df.iloc[4,1]), color = \"green\")\n",
    "\n",
    "print(x1, x2)\n",
    "\n",
    "plt.title('Logistic Regression Decision Boundary')\n",
    "plt.xlabel('Feature 1')\n",
    "plt.ylabel('Feature 2')\n",
    "plt.show()\n"
   ]
