  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "2ec0fd58",
   "metadata": {},
   "outputs": [],
   "source": [
    "def gradient_descent(X, y, learning_rate, nbr_steps):\n",
    "    intercept = np.ones((X.shape[0], 1))\n",
    "    X = np.hstack((intercept, X))\n",
    "    weights = np.zeros(X.shape[1])\n",
    "    m = len(df)\n",
    "    for step in range(nbr_steps):\n",
    "        scores = np.dot(X, weights)\n",
    "        hypothesis = Sigmoid(scores)\n",
    "        output_error_signal = hypothesis - y\n",
    "        \n",
    "        gradient = np.dot(X.T, output_error_signal)\n",
    "        \n",
    "        weights -= (learning_rate/m) * gradient\n",
    "        \n",
    "    return weights"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 64,
   "id": "22894913",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[-3.55481127  1.32855201]\n"
     ]
    }
   ],
   "source": [
    "weights = gradient_descent(df[['hours_studying']], df['pass_exam'], 0.001, 100000)\n",
    "print(weights)"
   ]
  },
