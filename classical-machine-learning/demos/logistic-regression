   "source": [
    "# Plotting Sigmoid function\n",
    "X = list(x * 0.1 for x in range(-100, 100))\n",
    "y = list(Sigmoid(x) for x in X )\n",
    "\n",
    "plt.plot(X, y)\n",
    "plt.show()"
   ]
